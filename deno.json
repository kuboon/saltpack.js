{
  "name": "@kuboon/saltpack",
  "version": "0.1.4",
  "exports": {
    ".": "./mod.ts",
    "./cli": "./src/cli.ts"
  },
  "tasks": {
    "dev": "deno run --watch mod.ts",
    "release": "deno run --allow-read --allow-write --allow-run=git scripts/release.ts"
  },
  "imports": {
    "@std/cli": "jsr:@std/cli@^1.0.25",
    "@std/io": "jsr:@std/io@^0.225.2",
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "tweetnacl": "npm:tweetnacl@^1.0.3",
    "@msgpack/msgpack": "npm:@msgpack/msgpack@^3.0.0-beta2"
  },
  "test": {
    "permissions": {
      "run": [
        "deno"
      ]
    }
  }
}
