{
  "name": "@kuboon/saltpack",
  "tasks": {
    "dev": "deno run --watch mod.ts",
    "check:all": "deno check && deno lint && deno test -P && deno fmt",
    "release": "deno run --allow-read --allow-write --allow-run=git scripts/release.ts"
  },
  "imports": {
    "@std/cli": "jsr:@std/cli@^1.0.25",
    "@std/io": "jsr:@std/io@^0.225.2",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "tweetnacl": "npm:tweetnacl@^1.0.3",
    "@msgpack/msgpack": "npm:@msgpack/msgpack@^3.1.3"
  },
  "version": "0.1.7",
  "exports": {
    ".": "./mod.ts",
    "./cli": "./src/cli.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "mod.ts",
      "src/"
    ]
  },
  "test": {
    "permissions": {
      "run": [
        "deno"
      ]
    }
  }
}
